
.hd-list {
  display: flex;
  flex-direction: row;
  align-items: center;
  background-color: #fff;
  height: 80rpx;
  padding: 0 20rpx;
  margin-bottom: 30rpx;
}

.hd-list .item-name {
  width: 150rpx;
}

.hd-list .item-content {
  flex: 1;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.hd-list .asn-code {
  flex: 1;
}

.hd-list .asn-btn {
  width: 150rpx;
}

.hd-list .asn-input {
  flex: 1;

}


.hd-list:last-child .item-name {
  position: relative;
  width: 120rpx;
  height: 60rpx;
}

.hd-list:last-child .item-name:after {
  content: '';
  position: absolute;
  top: 0;
  left: 20rpx;
  width: 60rpx;
  height: 60rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: cover;
}
.hd-list:last-child input{
  font-size: 30rpx;
}
.delFill{
  width:120rpx;
  height: 60rpx;
  margin: 0 40rpx;
}
.delFill view{
  width:40rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  margin: 10rpx auto;
  font-size: 36rpx;
  color: #666;
}

.hd-list:last-child .item-name:before {
  content: '';
  position: absolute;
  top: 0;
  right: 10rpx;
  width: 2rpx;
  height: 100%;
