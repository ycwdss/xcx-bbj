}
.img-add {
  position: relative;
  width: 160rpx;
  height: 160rpx;
  border: 1px solid #e4e4e4;
  box-sizing: border-box;
}

.img-add:before {
  width: 4rpx;
  height: 80rpx;
}

.img-add:after {
  width: 80rpx;
  height: 4rpx;
}

.img-add:before, .img-add:after {
  content: " ";
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  background-color: #e4e4e4;
}

.img-wrap, .img-add {
  float: left;
}

.img{
  float: left;
  position: relative;
}
.del{
  position: absolute;
  top:0;
  right: 0;
  width: 30rpx;
  height: 30rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: cover;
}
.img-element {
  display: block;
  width: 160rpx;
  height: 160rpx;
}

.img-inner {
  position: absolute;
  z-index: 1;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  opacity: 0;

}

